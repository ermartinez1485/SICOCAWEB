    </managed-bean>
    <managed-bean>
        <managed-bean-name>matriculaBean</managed-bean-name>
        <managed-bean-class>cr.ac.una.icai.sicocaweb.beans.MatriculaBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>matriculaNotaBean</managed-bean-name>
        <managed-bean-class>cr.ac.una.icai.sicocaweb.beans.MatriculaNotaBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
